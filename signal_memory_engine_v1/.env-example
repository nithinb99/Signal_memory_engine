# === Pinecone ===
PINECONE_API_KEY=<your-pinecone-api-key>
PINECONE_ENVIRONMENT=<your-pinecone-environment>   # e.g. us-west1-gcp
PINECONE_INDEX=<your-pinecone-index-name>          # defaults to "signal-engine" if unset

# === OpenAI ===
OPENAI_API_KEY=<your-openai-api-key>
OPENAI_MODEL=<your-openai-model>                   # e.g. gpt-4o-mini

# === MLflow ===
MLFLOW_TRACKING_URI=                               # optional; falls back to ./mlruns
MLFLOW_EXPERIMENT_NAME=<your-experiment-name>      # defaults to "signal-memory-engine"

# === Agents ===
ENABLED_AGENTS=<your-agents>

# === Local DB (Signals) ===
SME_DB_PATH=<your-SME_DB_PATH>                     # defaults to ./data/signal.db if unset

# === Testing ===
SME_TEST_MODE=0                                    # optional test switch; default 0 (off)